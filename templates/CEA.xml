<templateSet group="CEA">
  <template name="ccon" value="console.log('cea:$CLIPPATH$:$METHOD$', { $END$ });" description="CEA Console from clipboard path" toReformat="false" toShortenFQNames="true" deactivated="true">
    <variable name="CLIPPATH" expression="substringBefore(clipboard(),&quot;.&quot;)" defaultValue="debug" alwaysStopAt="false" />
    <variable name="METHOD" expression="jsMethodName()" defaultValue="" alwaysStopAt="false" />
    <context>
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="sto" value="const $NAME$Styles = {&#10;  $END$,&#10;};" description="Style Object" toReformat="false" toShortenFQNames="true" deactivated="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
</templateSet>